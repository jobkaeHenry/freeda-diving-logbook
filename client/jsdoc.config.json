{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.(js|ts|jsx|tsx)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "better-docs/component",
    "better-docs/typescript"
  ],
  "templates": {
    "better-docs": {
      "name": "My React components"
    }
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "docs",
    "recurse": true
  },
  "docdash": {
    "static": false,
    "sort": false
  }
}
